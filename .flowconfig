[ignore]
.*/node_modules/react/.*
.*/node_modules/babel.*
.*/node_modules/fbjs/.*
.*test

[include]
./lib
index.js

[libs]

[options]
suppress_comment=\\(.\\|\n\\)*\\s*\\$FlowFixMe.*
